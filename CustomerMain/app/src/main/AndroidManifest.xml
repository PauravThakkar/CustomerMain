<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.customermain">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CustomerLogin"></activity>
        <activity android:name=".ManagerLogin" />
        <activity android:name=".LoginChoose" />
        <activity android:name=".ManagerMenu" />
        <activity android:name=".ManagerOrder" />
        <activity
            android:name=".ManagerMain"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CustomeMain" />
        <activity android:name=".CustomerProfile" />
        <activity android:name=".CustomerCart" />
        <activity android:name=".CustomerHistory" />
        <activity android:name=".CustomerHome" />
        <activity android:name=".OrderDetails"/>
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CanteenMenu" />

        <service android:name=".NotificationReceiver">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>